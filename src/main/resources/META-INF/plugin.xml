<idea-plugin>
    <id>org.example.plug</id>
    <name>Test</name>
    <vendor email="support@yourcompan.com">Test</vendor>
    <description>description description description description description</description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="JAVA"
                         displayName="Test insp 0"
                         groupName="Test"
                         enabledByDefault="true"
                         implementationClass="LombokUtilityClassInspection">
        </localInspection>

        <localInspection language="JAVA"
                         displayName="Test insp 1"
                         groupName="Test"
                         enabledByDefault="true"
                         implementationClass="StepMethodAnnotationInspection">
        </localInspection>

        <localInspection language="JAVA"
                         displayName="Test insp 2"
                         groupName="Test"
                         enabledByDefault="true"
                         implementationClass="StepAnnotationInspection">
        </localInspection>
    </extensions>

    <actions></actions>
</idea-plugin>